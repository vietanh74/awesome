{
  "name": "awesome",
  "private": true,
  "version": "0.0.0",
  "homepage": "https://vietanh74.github.io/awesome",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist -t true",
    "types-check": "vue-tsc --noEmit",
    "lint:script": "eslint -c .eslintrc.js \"./src/**/*.{js,ts,vue}\" --ignore-path .gitignore .",
    "lint:prettier": "prettier --check \"src/**/*+(js|ts|vue|scss)\"",
    "lint:style": "stylelint src/**/*.{css,scss}",
    "lint": "npm run lint:script && npm run lint:prettier && npm run lint:style",
    "lintfix": "prettier --write --list-different \"src/**/*+(js|ts|vue|scss)\" && npm run lint:script -- --fix && npm run lint:style -- --fix",
    "prepare": "husky"
  },
  "dependencies": {
    "@ant-design/icons-vue": "^7.0.1",
    "ant-design-vue": "^4.2.6",
    "axios": "^1.7.9",
    "date-chinese": "^2.1.4",
    "date-fns": "^4.1.0",
    "gh-pages": "^6.1.1",
    "lodash-es": "^4.17.21",
    "pinia": "^3.0.1",
    "tailwindcss": "^3.4.17",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.17.19",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "eslint": "^8.57.1",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-vue": "^9.32.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.2.10",
    "postcss": "^8.5.3",
    "postcss-html": "^1.8.0",
    "prettier": "^3.5.1",
    "rollup-plugin-copy": "^3.5.0",
    "sass": "^1.85.0",
    "stylelint": "^16.14.1",
    "stylelint-config-prettier-scss": "^1.0.0",
    "stylelint-config-recommended-vue": "^1.6.0",
    "stylelint-config-standard-scss": "^14.0.0",
    "typescript": "~5.5.4",
    "vite": "^6.1.1",
    "vite-svg-loader": "^5.1.0",
    "vue-tsc": "^2.2.2"
  },
  "lint-staged": {
    "*.{js,ts,vue,css,scss}": "npm run lint"
  }
}
