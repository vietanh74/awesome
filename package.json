{
  "name": "awesome",
  "private": true,
  "version": "0.0.0",
  "homepage": "https://vietanh74.github.io/awesome",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist",
    "types-check": "vue-tsc --noEmit",
    "lint:script": "eslint -c .eslintrc.js \"./src/**/*.{js,ts,vue}\" --ignore-path .gitignore .",
    "lint:prettier": "prettier --check \"src/**/*+(js|ts|vue|scss)\"",
    "lint:style": "stylelint src/**/*.{css,scss}",
    "lint": "npm run lint:script && npm run lint:prettier && npm run lint:style",
    "lintfix": "prettier --write --list-different \"src/**/*+(js|ts|vue|scss)\" && npm run lint:script -- --fix && npm run lint:style -- --fix",
    "prepare": "husky install"
  },
  "dependencies": {
    "@ant-design/icons-vue": "^6.1.0",
    "ant-design-vue": "^4.2.3",
    "axios": "^1.7.2",
    "date-chinese": "^2.1.4",
    "date-fns": "^2.28.0",
    "gh-pages": "^4.0.0",
    "lodash-es": "^4.17.21",
    "pinia": "^2.0.13",
    "save-dev": "^0.0.1-security",
    "tailwindcss": "^3.0.24",
    "vue": "^3.2.25",
    "vue-router": "^4.0.14"
  },
  "devDependencies": {
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^17.0.23",
    "@typescript-eslint/eslint-plugin": "^5.19.0",
    "@typescript-eslint/parser": "^5.19.0",
    "@vitejs/plugin-vue": "^2.3.0",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^10.0.0",
    "eslint": "^8.13.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.6.0",
    "husky": "^7.0.4",
    "less": "^4.1.2",
    "lint-staged": "^12.3.7",
    "postcss": "^8.4.12",
    "postcss-html": "^1.3.1",
    "prettier": "^2.6.2",
    "rollup-plugin-copy": "^3.4.0",
    "sass": "^1.50.0",
    "stylelint": "^14.6.1",
    "stylelint-config-prettier-scss": "^0.0.1",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-config-standard-scss": "^3.0.0",
    "typescript": "^4.5.4",
    "vite": "^2.9.13",
    "vite-plugin-imp": "^2.1.8",
    "vue-tsc": "^1.0.9"
  },
  "lint-staged": {
    "*.{js,ts,vue,css,scss}": "npm run lint"
  }
}
